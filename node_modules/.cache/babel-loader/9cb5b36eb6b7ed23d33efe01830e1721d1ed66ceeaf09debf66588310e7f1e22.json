{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      product: null,\n      loading: true,\n      errorMessage: null,\n      backendUrl: 'http://localhost:3000'\n    };\n  },\n  async created() {\n    const productId = this.$route.params.id;\n    if (!/^\\d+$/.test(productId)) {\n      this.errorMessage = 'Некорректный ID';\n      this.loading = false;\n      return;\n    }\n    await this.fetchProduct(productId);\n  },\n  methods: {\n    async fetchProduct(productId) {\n      try {\n        const response = await fetch(`${this.backendUrl}/api/view-pitanie/${productId}`, {\n          method: 'GET',\n          credentials: 'include' // Отправляем куки с токеном\n        });\n        if (!response.ok) {\n          const text = await response.text();\n          console.error('Server response:', text);\n          if (response.status === 401) {\n            this.$router.push('/login');\n            return;\n          }\n          if (response.status === 404) {\n            throw new Error('Продукт не найден');\n          }\n          throw new Error(`HTTP error! Status: ${response.status}`);\n        }\n        const data = await response.json();\n        this.product = data;\n      } catch (err) {\n        console.error('Ошибка при получении данных продукта:', err);\n        this.errorMessage = err.message;\n      } finally {\n        this.loading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","product","loading","errorMessage","backendUrl","created","productId","$route","params","id","test","fetchProduct","methods","response","fetch","method","credentials","ok","text","console","error","status","$router","push","Error","json","err","message"],"sources":["/home/vas/zoo/vueapp/src/components/ViewPitanie.vue"],"sourcesContent":["<template>\n  <div class=\"app-container\">\n    <h1 class=\"page-title\">Просмотр продукта</h1>\n    <div v-if=\"loading\" class=\"loading-message\">Загрузка данных...</div>\n    <div v-else-if=\"errorMessage\" class=\"error-message\">{{ errorMessage }}</div>\n    <div v-else-if=\"product\" class=\"product-details\">\n      <div class=\"product-image-container\">\n        <img v-if=\"product.img\" :src=\"`${backendUrl}${product.img}`\" alt=\"Product Image\" class=\"product-image\" />\n        <span v-else>Нет изображения</span>\n      </div>\n      <div class=\"product-info\">\n        <p><strong>Название:</strong> {{ product.name }}</p>\n        <p><strong>Вид:</strong> {{ product.vid }}</p>\n        <p><strong>Бренд:</strong> {{ product.brand }}</p>\n        <p><strong>Вес:</strong> {{ product.weight }} г</p>\n        <p><strong>Описание:</strong> {{ product.description }}</p>\n        <p><strong>Цена:</strong> {{ product.price }} ₽</p>\n        <p>\n          <strong>В наличии:</strong>\n          <span :class=\"['status', product.in_stock ? 'available' : 'unavailable']\">\n            {{ product.in_stock ? 'Да' : 'Нет' }}\n          </span>\n        </p>\n      </div>\n      <div class=\"action-buttons\">\n        <button class=\"btn-secondary\" @click=\"$router.push('/pitanie')\">Вернуться к списку</button>\n      </div>\n    </div>\n    <div v-else class=\"error-message\">Не удалось загрузить данные о продукте.</div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      product: null,\n      loading: true,\n      errorMessage: null,\n      backendUrl: 'http://localhost:3000',\n    };\n  },\n  async created() {\n    const productId = this.$route.params.id;\n    if (!/^\\d+$/.test(productId)) {\n      this.errorMessage = 'Некорректный ID';\n      this.loading = false;\n      return;\n    }\n    await this.fetchProduct(productId);\n  },\n  methods: {\n    async fetchProduct(productId) {\n      try {\n        const response = await fetch(`${this.backendUrl}/api/view-pitanie/${productId}`, {\n          method: 'GET',\n          credentials: 'include', // Отправляем куки с токеном\n        });\n        if (!response.ok) {\n          const text = await response.text();\n          console.error('Server response:', text);\n          if (response.status === 401) {\n            this.$router.push('/login');\n            return;\n          }\n          if (response.status === 404) {\n            throw new Error('Продукт не найден');\n          }\n          throw new Error(`HTTP error! Status: ${response.status}`);\n        }\n        const data = await response.json();\n        this.product = data;\n      } catch (err) {\n        console.error('Ошибка при получении данных продукта:', err);\n        this.errorMessage = err.message;\n      } finally {\n        this.loading = false;\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Стили остаются без изменений */\n</style>"],"mappings":";AAiCA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,IAAI;MACbC,OAAO,EAAE,IAAI;MACbC,YAAY,EAAE,IAAI;MAClBC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,MAAMC,SAAQ,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,EAAE;IACvC,IAAI,CAAC,OAAO,CAACC,IAAI,CAACJ,SAAS,CAAC,EAAE;MAC5B,IAAI,CAACH,YAAW,GAAI,iBAAiB;MACrC,IAAI,CAACD,OAAM,GAAI,KAAK;MACpB;IACF;IACA,MAAM,IAAI,CAACS,YAAY,CAACL,SAAS,CAAC;EACpC,CAAC;EACDM,OAAO,EAAE;IACP,MAAMD,YAAYA,CAACL,SAAS,EAAE;MAC5B,IAAI;QACF,MAAMO,QAAO,GAAI,MAAMC,KAAK,CAAC,GAAG,IAAI,CAACV,UAAU,qBAAqBE,SAAS,EAAE,EAAE;UAC/ES,MAAM,EAAE,KAAK;UACbC,WAAW,EAAE,SAAS,CAAE;QAC1B,CAAC,CAAC;QACF,IAAI,CAACH,QAAQ,CAACI,EAAE,EAAE;UAChB,MAAMC,IAAG,GAAI,MAAML,QAAQ,CAACK,IAAI,CAAC,CAAC;UAClCC,OAAO,CAACC,KAAK,CAAC,kBAAkB,EAAEF,IAAI,CAAC;UACvC,IAAIL,QAAQ,CAACQ,MAAK,KAAM,GAAG,EAAE;YAC3B,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;YAC3B;UACF;UACA,IAAIV,QAAQ,CAACQ,MAAK,KAAM,GAAG,EAAE;YAC3B,MAAM,IAAIG,KAAK,CAAC,mBAAmB,CAAC;UACtC;UACA,MAAM,IAAIA,KAAK,CAAC,uBAAuBX,QAAQ,CAACQ,MAAM,EAAE,CAAC;QAC3D;QACA,MAAMrB,IAAG,GAAI,MAAMa,QAAQ,CAACY,IAAI,CAAC,CAAC;QAClC,IAAI,CAACxB,OAAM,GAAID,IAAI;MACrB,EAAE,OAAO0B,GAAG,EAAE;QACZP,OAAO,CAACC,KAAK,CAAC,uCAAuC,EAAEM,GAAG,CAAC;QAC3D,IA