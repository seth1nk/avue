{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      pet: null,\n      loading: true,\n      errorMessage: null,\n      backendUrl: 'http://localhost:3000'\n    };\n  },\n  async created() {\n    const petId = this.$route.params.id;\n    if (!/^\\d+$/.test(petId)) {\n      this.errorMessage = 'Некорректный ID';\n      this.loading = false;\n      return;\n    }\n    await this.fetchPet(petId);\n  },\n  methods: {\n    async fetchPet(petId) {\n      try {\n        const response = await fetch(`${this.backendUrl}/api/view-pet/${petId}`, {\n          method: 'GET',\n          credentials: 'include' // Отправляем куки с токеном\n        });\n        if (!response.ok) {\n          const text = await response.text(); // Проверяем тело ответа\n          console.error('Server response:', text);\n          if (response.status === 401) {\n            this.$router.push('/login'); // Перенаправляем на логин при отсутствии авторизации\n            return;\n          }\n          if (response.status === 404) {\n            throw new Error('Питомец не найден');\n          }\n          throw new Error(`HTTP error! Status: ${response.status}`);\n        }\n        const data = await response.json();\n        this.pet = data;\n      } catch (err) {\n        console.error('Ошибка при получении данных питомца:', err);\n        this.errorMessage = err.message;\n      } finally {\n        this.loading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","pet","loading","errorMessage","backendUrl","created","petId","$route","params","id","test","fetchPet","methods","response","fetch","method","credentials","ok","text","console","error","status","$router","push","Error","json","err","message"],"sources":["/home/vas/zoo/vueapp/src/components/ViewPet.vue"],"sourcesContent":["<template>\n  <div class=\"app-container\">\n    <h1 class=\"page-title\">Просмотр питомца</h1>\n    <div v-if=\"loading\" class=\"loading-message\">Загрузка данных...</div>\n    <div v-else-if=\"errorMessage\" class=\"error-message\">{{ errorMessage }}</div>\n    <div v-else-if=\"pet\" class=\"pet-details\">\n      <div class=\"pet-image-container\">\n        <img v-if=\"pet.img\" :src=\"`${backendUrl}${pet.img}`\" alt=\"Pet Image\" class=\"pet-image\" />\n        <span v-else>Нет изображения</span>\n      </div>\n      <div class=\"pet-info\">\n        <p><strong>Имя:</strong> {{ pet.name }}</p>\n        <p><strong>Вид:</strong> {{ pet.species }}</p>\n        <p><strong>Возраст:</strong> {{ pet.age }} лет</p>\n        <p><strong>Пол:</strong> {{ pet.gender }}</p>\n        <p><strong>Описание:</strong> {{ pet.description }}</p>\n        <p><strong>Цена:</strong> {{ pet.price }} ₽</p>\n        <p>\n          <strong>Доступен:</strong>\n          <span :class=\"['status', pet.available ? 'available' : 'unavailable']\">\n            {{ pet.available ? 'Да' : 'Нет' }}\n          </span>\n        </p>\n      </div>\n      <div class=\"action-buttons\">\n        <button class=\"btn-secondary\" @click=\"$router.push('/pets')\">Вернуться к списку</button>\n      </div>\n    </div>\n    <div v-else class=\"error-message\">Не удалось загрузить данные о питомце.</div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      pet: null,\n      loading: true,\n      errorMessage: null,\n      backendUrl: 'http://localhost:3000',\n    };\n  },\n  async created() {\n    const petId = this.$route.params.id;\n    if (!/^\\d+$/.test(petId)) {\n      this.errorMessage = 'Некорректный ID';\n      this.loading = false;\n      return;\n    }\n    await this.fetchPet(petId);\n  },\n  methods: {\n    async fetchPet(petId) {\n      try {\n        const response = await fetch(`${this.backendUrl}/api/view-pet/${petId}`, {\n          method: 'GET',\n          credentials: 'include', // Отправляем куки с токеном\n        });\n        if (!response.ok) {\n          const text = await response.text(); // Проверяем тело ответа\n          console.error('Server response:', text);\n          if (response.status === 401) {\n            this.$router.push('/login'); // Перенаправляем на логин при отсутствии авторизации\n            return;\n          }\n          if (response.status === 404) {\n            throw new Error('Питомец не найден');\n          }\n          throw new Error(`HTTP error! Status: ${response.status}`);\n        }\n        const data = await response.json();\n        this.pet = data;\n      } catch (err) {\n        console.error('Ошибка при получении данных питомца:', err);\n        this.errorMessage = err.message;\n      } finally {\n        this.loading = false;\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Стили остаются без изменений */\n</style>"],"mappings":";AAiCA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,GAAG,EAAE,IAAI;MACTC,OAAO,EAAE,IAAI;MACbC,YAAY,EAAE,IAAI;MAClBC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,MAAMC,KAAI,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,EAAE;IACnC,IAAI,CAAC,OAAO,CAACC,IAAI,CAACJ,KAAK,CAAC,EAAE;MACxB,IAAI,CAACH,YAAW,GAAI,iBAAiB;MACrC,IAAI,CAACD,OAAM,GAAI,KAAK;MACpB;IACF;IACA,MAAM,IAAI,CAACS,QAAQ,CAACL,KAAK,CAAC;EAC5B,CAAC;EACDM,OAAO,EAAE;IACP,MAAMD,QAAQA,CAACL,KAAK,EAAE;MACpB,IAAI;QACF,MAAMO,QAAO,GAAI,MAAMC,KAAK,CAAC,GAAG,IAAI,CAACV,UAAU,iBAAiBE,KAAK,EAAE,EAAE;UACvES,MAAM,EAAE,KAAK;UACbC,WAAW,EAAE,SAAS,CAAE;QAC1B,CAAC,CAAC;QACF,IAAI,CAACH,QAAQ,CAACI,EAAE,EAAE;UAChB,MAAMC,IAAG,GAAI,MAAML,QAAQ,CAACK,IAAI,CAAC,CAAC,EAAE;UACpCC,OAAO,CAACC,KAAK,CAAC,kBAAkB,EAAEF,IAAI,CAAC;UACvC,IAAIL,QAAQ,CAACQ,MAAK,KAAM,GAAG,EAAE;YAC3B,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC,EAAE;YAC7B;UACF;UACA,IAAIV,QAAQ,CAACQ,MAAK,KAAM,GAAG,EAAE;YAC3B,MAAM,IAAIG,KAAK,CAAC,mBAAmB,CAAC;UACtC;UACA,MAAM,IAAIA,KAAK,CAAC,uBAAuBX,QAAQ,CAACQ,MAAM,EAAE,CAAC;QAC3D;QACA,MAAMrB,IAAG,GAAI,MAAMa,QAAQ,CAACY,IAAI,CAAC,CAAC;QAClC,IAAI,CAACxB,GAAE,GAAID,IAAI;MACjB,EAAE,OAAO0B,GAAG,EAAE;QACZP,OAAO,CAACC,KAAK,CAAC,sCAAsC,EAAEM,GAAG,CAAC;QAC1D